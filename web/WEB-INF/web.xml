<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>deudas</servlet-name>
        <servlet-class>servlet.deudas</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>productos</servlet-name>
        <servlet-class>servlet.productos</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>verHistorial</servlet-name>
        <servlet-class>servlet.verHistorial</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>servlet.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>categoriasControlador</servlet-name>
        <servlet-class>servlet.categoriasControlador</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>productosControlador</servlet-name>
        <servlet-class>servlet.productosControlador</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>proveedorControlador</servlet-name>
        <servlet-class>servlet.proveedorControlador</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>fiadosControlador</servlet-name>
        <servlet-class>servlet.fiadosControlador</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>carro</servlet-name>
        <servlet-class>servlet.carro</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>test</servlet-name>
        <servlet-class>servlet.test</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>deudas</servlet-name>
        <url-pattern>/deudas</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>productos</servlet-name>
        <url-pattern>/productos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>verHistorial</servlet-name>
        <url-pattern>/verHistorial</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>categoriasControlador</servlet-name>
        <url-pattern>/categoriasControlador</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>productosControlador</servlet-name>
        <url-pattern>/productosControlador</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>proveedorControlador</servlet-name>
        <url-pattern>/proveedorControlador</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>fiadosControlador</servlet-name>
        <url-pattern>/fiadosControlador</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>carro</servlet-name>
        <url-pattern>/carro</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>test</servlet-name>
        <url-pattern>/test</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
